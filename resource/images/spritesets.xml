<?xml version="1.0"?> 

<manifest>
  <spriteset name="dirt">
    <sprite img="resource/images/tiles/dirt-block.png"/>
  </spriteset>
  
  <group name="fences">
    <sprite id="front" img="resource/images/props/fence-front.png" ay="0.82"/>
    <sprite id="side" img="resource/images/props/fence-front.png" ay="0.82"/>
    
    <spriteset name="fencefront">
      <faces n="front" e="side" s="front" w="side" />
    </spriteset>
    
    <spriteset name="fenceSide">
      <faces n="side" e="front" s="side" w="front" />
    </spriteset>	
  </group>

  <spriteset name="ghost-blue">
    <sprite id="lf" img="resource\images\players\ghost\blue\left-front.png" ay="0.8" />
    <sprite id="lb" img="resource\images\players\ghost\blue\left-back.png" ay="0.8" />
    <sprite id="rf" img="resource\images\players\ghost\blue\right-front.png" ay="0.8" />
    <sprite id="rb" img="resource\images\players\ghost\blue\right-back.png" ay="0.8" />
    <faces n="rb" e="lf" s="rf" w="lb" />
  </spriteset>

</manifest>